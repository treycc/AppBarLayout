<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.treycc.appbarlayout">

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".FirstActivity"
            android:theme="@style/Translucent" />
        <activity
            android:name=".SecondActivity"
            android:theme="@style/Translucent" />
        <activity
            android:name=".ReavalActivity"
            android:theme="@style/Translucent" />
        <activity
            android:name=".NestScrollActivity"
            android:theme="@style/Translucent" />

        <activity
            android:name=".SlidingPaneLayoutActivity"/>

        <activity
            android:name=".SlidingSecondAcitivity"
            android:theme="@style/Translucent" />

        <activity android:name=".RecylerViewActivity"/>

    </application>

</manifest>